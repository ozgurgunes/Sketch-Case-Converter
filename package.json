{
  "name": "case-converter",
  "description": "Converts case of text in selected layers, symbol overrides or even all overrides of all instances of a symbol to 'Sentence case', 'Title Case', 'UPPER CASE' or 'lower case'.",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com",
  "homepage": "https://github.com/ozgurgunes/Sketch-Case-Converter",
  "license": "MIT",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozgurgunes/Sketch-Case-Converter.git"
  },
  "bugs": {
    "url": "https://github.com/ozgurgunes/Sketch-Case-Converter/issues"
  },
  "engines": {
    "sketch": ">=52"
  },
  "skpm": {
    "name": "Case Converter",
    "manifest": "src/manifest.json",
    "main": "case-converter.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/babel-preset": "^0.2.2",
    "@skpm/builder": "^0.7.7",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-sketch": "^0.2.4",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "@ozgurgunes/sketch-plugin-analytics": "^0.5.0",
    "@ozgurgunes/sketch-plugin-ui": "^0.5.0",
    "langmap": "0.0.16"
  }
}
